<form method="post" action="{{ path('trajet_delete', {'idtrajet': trajet.idtrajet}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ trajet.idtrajet) }}">
    <button class="btn">Delete</button>
</form>

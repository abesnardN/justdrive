<form method="post" action="{{ path('occupant_delete', {'fktrajet': occupant.fktrajet}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ occupant.fktrajet) }}">
    <button class="btn">Delete</button>
</form>

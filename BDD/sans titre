Site : idSite INT,label VARCHAR(150)
DF1, 0N Adresse, _11 Site
DF, 0N Pays, _11 Adresse
Adresse : idAdresse INT,numRue VARCHAR(4),nomRue VARCHAR(50),ville VARCHAR(50),codePostal VARCHAR(5),latitude DECIMAL,longitude DECIMAL
Depart, 0N Adresse, _11 Trajet
Arrive, 0N Adresse, _11 Trajet

Pays : idPays CHAR(3),libelle VARCHAR(25)
Conducteur, 0N User, _11 Trajet
Trajet : idTrajet INT,	adresseDepart INT,addresseArrive INT,fkVehicule INT,dateDepart DATETIME NOT NULL,dateArrive DATETIME NOT NULL,kilometrage DECIMAL,commentaireConducteur VARCHAR(150),commentaireInterne VARCHAR(150)
DF5, 0N EtatVehicule, _11 Trajet
EtatVehicule :idEtat INT,libelle VARCHAR(50)
User:idUser INT,nom VARCHAR(50),prenom VARCHAR(50),permis DATETIME,urlPermis VARCHAR(50),dateInscription DATETIME NOT NULL,mail VARCHAR(50),numTel VARCHAR(13)

DF7, 0N Vehicule, _11 Trajet
Vehicule:idVehicule INT,modele VARCHAR(50),kilometrage DECIMAL,marque VARCHAR(50),nbPlace INT NOT NULL,immatriculation VARCHAR(12),datePremiereCirculation date,
DF2, 0N EtatVehicule, _11 Vehicule
Occupant, 1N Trajet, 1N User, 1N EtatVehicule
Cle: idCle INT
DF6, 0N Site, _11 Cle

DF8, 0N User, _11 Cle
DF9, 0N Vehicule, _11 Cle
Planninng : idCle INT, planning VARCHAR(265), fkVehicule INT, annee INT NOT NULL
:::
